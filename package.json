{
  "name": "ga-island",
  "version": "2.0.0",
  "description": "Genetic Algorithm with 'island' Diversity support",
  "keywords": [
    "genetic",
    "algorithm",
    "genetic algorithm",
    "artificial evolution",
    "evolve",
    "typescript"
  ],
  "author": "Beeno Tung <aabbcc1241@yahoo.com.hk> (https://beeno-tung.surge.sh)",
  "license": "BSD-2-Clause",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "demo:build": "cd examples && parcel build islandHop.html && cd ..",
    "demo:serve": "run-s demo:build demo:http-server",
    "demo:http-server": "http-server examples/dist",
    "demo:publish": "run-s demo:build demo:cp surge",
    "demo:cp": "cd examples/dist && cp islandHop.html index.html && cd ../..",
    "surge": "surge examples/dist https://ga-island-demo.surge.sh",
    "clean": "rimraf dist",
    "format": "run-s prettier lint",
    "prettier": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "lint": "tslint -p . --fix",
    "build": "run-s clean tsc mk-package",
    "tsc": "tsc",
    "mk-package": "mk-package",
    "test": "run-p mocha format build",
    "mocha": "ts-mocha test/ga-island.spec.ts",
    "prepublishOnly": "run-s mocha build"
  },
  "husky": {
    "hook": {
      "pre-commit": "npm run format"
    }
  },
  "directories": {
    "test": "test"
  },
  "devDependencies": {
    "@types/chai": "^4.2.7",
    "@types/mocha": "^5.2.7",
    "@types/node": "*",
    "chai": "^4.2.0",
    "cssnano": "^4.1.10",
    "http-server": "^0.12.3",
    "husky": "^3.1.0",
    "mk-package": "^0.2.0",
    "mocha": "^6.2.2",
    "npm-run-all": "^4.1.5",
    "parcel": "^1.12.4",
    "prettier": "^1.19.1",
    "rimraf": "^3.0.0",
    "surge": "^0.21.6",
    "ts-mocha": "^8.0.0",
    "ts-node": "^8.5.4",
    "tslint": "^5.20.1",
    "tslint-config-prettier": "^1.18.0",
    "tslint-eslint-rules": "^5.4.0",
    "typescript": "^3.7.3"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/beenotung/ga-island.git"
  },
  "bugs": {
    "url": "https://github.com/beenotung/ga-island/issues"
  },
  "homepage": "https://github.com/beenotung/ga-island#readme"
}
